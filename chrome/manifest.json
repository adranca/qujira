{
    "manifest_version": 2,
    "default_locale": "en",

    "name": "Qujira",
    "version": "1.0",
    "description": "Quickly copy an issue's URL and summary",

    "content_scripts": [{
        "css": ["content.css"],
        "js": ["content.js"],
        "matches": ["https://*/browse/*"]
    }],

    "permissions": []
}